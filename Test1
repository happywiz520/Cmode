#include<stdio.h>
#include<string.h>
int main()
{

FILE *aws;
int c;
char text[100];
int i = 0;

aws = fopen("quiz9.txt","r");

while(1)
{
	c = fgetc(aws);
	if(feof(aws))
		break;
		
	text[i] = c;
	i++;

	printf(" %c = %x\n",c,c);
}
	fclose(aws);
	printf("\n");
	printf("%s",text);
	
	
	
	return(0);
}
